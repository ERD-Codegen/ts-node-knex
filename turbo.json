{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"clean": {
			"cache": false
		},
		"lint": {
			"outputs": []
		},
		"start": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"test": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^clean"],
			"outputs": ["build/**", "dist/**", ".next/**"]
		}
	}
}
