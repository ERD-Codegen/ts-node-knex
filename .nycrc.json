{
	"extends": "@istanbuljs/nyc-config-typescript",
	"all": true,
	"check-coverage": true,
	"reporter": "lcov",
	"report-dir": "./coverage",
	"skip-full": false,
	"temp-dir": "./.nyc_output",
	"exclude": [
		"output",
		"coverage",
		"**/error/APIError/**",
		"**/.eslintrc.base.cjs",
		"**/.eslintrc.cjs",
		"**/.prettierrc.cjs",
		"**/local.ts",
		"**/app.ts",
		"**/lambda.ts"
	]
}
